#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	int arr[]={ 1, 2, 5, 10, 20, 50, 100, 200, 500, 2000} ;
	while(t--){
	    int n;
	    cin>>n;
	    int i = 9;
	    int c=0;
	    while(n>0){
	        while(n>=arr[i]){
	            c++;
	            n = n - arr[i];
	            cout<<arr[i]<<" ";
	        }
	        i--;
	    }
	    cout<<'\n';
	    
	}
	
	return 0;
}
